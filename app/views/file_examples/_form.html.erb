<%= form_for(@file_example) do |f| %>
  <% if @file_example.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@file_example.errors.count, "error") %> prohibited this file_example from being saved:</h2>

      <ul>
      <% @file_example.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :system_example_id %><br>
    <%= f.collection_select :system_example_id, SystemExample.all, :id, :name, {:include_blank => true} %>
  </div>
  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :code %><br>
    <%= f.text_area :code %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
