<%= form_for(@file_example) do |f| %>
  <%= render 'shared/form_error_message_listing', object: f.object %>

  <p>
    <%= f.label :system_example_id %><br>
    <%= f.collection_select :system_example_id, SystemExample.all, :id, :name, {:include_blank => true} %>
  </p>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :code %><br>
    <%= f.text_area :code %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= form_tag file_examples_read_file_path, :multipart => true do %>
  <p>
    <%= label_tag :system_example_id %>
    <%= collection_select(:file_example, :system_example_id, SystemExample.all, :id, :name, prompt: true) %>
    <label for="file">Upload text File</label> <%= file_field_tag "file" %>
    <%= submit_tag %>
  </p>
<% end %>
