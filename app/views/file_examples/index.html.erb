<h1>Listing file_examples</h1>

<table>
  <thead>
    <tr>
      <th>System example</th>
      <th>Name</th>
      <th>Code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @file_examples.each do |file_example| %>
      <tr>
        <td><%= file_example.system_example %></td>
        <td><%= file_example.name %></td>
        <td><%= file_example.code %></td>
        <td><%= link_to 'Show', file_example %></td>
        <td><%= link_to 'Edit', edit_file_example_path(file_example) %></td>
        <td><%= link_to 'Destroy', file_example, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New File example', new_file_example_path %>
