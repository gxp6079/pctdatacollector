<%= form_for(@system_example) do |f| %>
  <%= render 'shared/form_error_message_listing', object: f.object %>

  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :is_for_training %><br>
    <%= f.check_box :is_for_training %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
