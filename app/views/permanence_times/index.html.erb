<h1>Listing permanence_times</h1>

<table>
  <thead>
    <tr>
      <th>Task</th>
      <th>System example</th>
      <th>File example</th>
      <th>User</th>
      <th>Seconds</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @permanence_times.each do |permanence_time| %>
      <tr>
        <td><%= permanence_time.task %></td>
        <td><%= permanence_time.system_example %></td>
        <td><%= permanence_time.file_example %></td>
        <td><%= permanence_time.user %></td>
        <td><%= permanence_time.seconds %></td>
        <td><%= link_to 'Show', permanence_time %></td>
        <td><%= link_to 'Edit', edit_permanence_time_path(permanence_time) %></td>
        <td><%= link_to 'Destroy', permanence_time, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Permanence time', new_permanence_time_path %>
